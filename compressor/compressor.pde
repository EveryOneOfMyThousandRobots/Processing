import java.util.TreeMap;
TreeMap<String, Integer> dct = new TreeMap<String, Integer>();
int dct_id = 0;

void compress() {
  int len = 10;
  while (len > 5) {
    int i = 0;
    while (i < toCompress.length() - len) {

      String search = toCompress.substring(i, i + len);
      if (search.indexOf("¬") >= 0) {
        i += 1;
        continue;
      }
      
      int found = 0;
      for (int j = len; j + len < toCompress.length(); j += 1) {
        if (toCompress.substring(j, j + len).equals(search)) {
          found += 1;
        }
      }
      if (found > 4) {
        println("found " + found + " of \"" + search + "\"");
        dct_id += 1;
        dct.put(search, dct_id);
        toCompress = toCompress.replaceAll(search, "¬"+Integer.toHexString(dct_id)+"¬");
      }
      i += 1;
    }
    len -= 1;
  }
}

void setup() {
  noLoop();
  println(toCompress + " " + toCompress.length());
  compress();
  println(toCompress + " " + toCompress.length());
  println(dct);
}

